<div class="py-4 py-lg-6 studentDash-bg">
  <div class="container">
    <div class="row">
      <div class="col-lg-12 col-md-12 col-12">
        <div>
          <h1 class="text-white mb-1 display-4">Student Dashboard</h1>
          <p class="mb-0 text-white lead">Revealing all the paths you are currently walking</p>
        </div>
      </div>
    </div>
  </div>
</div>
<div class = "container studentDash-content" >
  <!-- Content -->
  <div class="row">
    <% for (let i = 0; i < user.courses.length; i++) { %>
    <div class="col-lg-3 col-md-6 col-12">
      <!-- Card -->
      <div class="card  mb-4 card-hover">
        <!-- Card Body -->
        <div class="card-body">
          <h4 class="mb-2 text-truncate-line-2 "><a href="course/<%=user.courses[i].id%> " class="text-inherit">
            <%=user.courses[i].title %>
            </a></h4>
          <!-- List -->
          <ul class="mb-3 list-inline">
            <li class="list-inline-item"><i class="far fa-clock mr-1"></i><%=user.courses[i].time %></li>
            <li class="list-inline-item">
              <%=user.courses[i].difficulty %>
            </li>
          </ul>
        </div>
        <!-- Card Footer -->
        <div class="card-footer">
          <div class="row align-items-center no-gutters">
            <div class="col-auto">
              <a href="#!" class="text-muted" data-toggle="tooltip" data-placement="top" title="Add to Bookmarks">
                <i class="fe fe-bookmark  "></i>
              </a>
            </div>
          </div>
        </div>
      </div>
    </div>
    <% } %>
      </div>
    </div>